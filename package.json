{
  "name": "@team-griffin/redux-clipboard",
  "version": "1.3.0",
  "main": "dist/cjs/redux-clipboard.js",
  "module": "dist/es/redux-clipboard.js",
  "jsnext:main": "lib/index.js",
  "repository": "git@github.com:team-griffin/redux-clipboard.git",
  "author": "Joshua Thornton <joshua.thornton@heg.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "install-self-peers -- --ignore-scripts",
    "prepublish": "npm run build",
    "transpile": "babel src -d lib --copy-files",
    "lint": "npm run lint:js && npm run flow",
    "lint:js": "eslint -c ./config/.eslintrc.js 'src/**/*.js'",
    "lint:js:fix": "eslint --fix -c ./config/.eslintrc.js 'src/**/*.js'",
    "flow": "flow check --show-all-errors; test $? -eq 0 -o $? -eq 2",
    "build:dev": "cross-env NODE_ENV=development rollup --config ./rollup/development.js",
    "build:prod": "cross-env NODE_ENV=production rollup --config ./rollup/production.js",
    "build:entry": "cross-env NODE_ENV=production rollup --config ./rollup/entry.js",
    "build": "yarn build:dev && yarn build:prod && yarn build:entry",
    "test": "cross-env NODE_ENV=test mocha --require babel-polyfill --compilers js:babel-register 'src/**/__tests__/*' -R spec",
    "ci": "npm run lint && npm run test && npm run build",
    "postmerge": "yarn",
    "dockerfile:ci": "dockerscript ./docker/ci/dockerfile.js ./docker/ci/Dockerfile"
  },
  "dependencies": {
    "@team-griffin/install-self-peers": "^1.1.1",
    "inversify-binding-decorators": "^3.0.0",
    "inversify-vanillajs-helpers": "^2.0.0",
    "partial-application-tree": "^0.5.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@team-griffin/eslint-config": "^3.0.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^8.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "dockerscript": "^0.0.6",
    "eslint": "^4.13.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "most-adjunct": "^0.9.0",
    "redux-mock-store": "^1.2.2",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-memory": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify-es": "^0.0.1",
    "sinon": "^4.1.3"
  },
  "peerDependencies": {
    "most": "^1.7.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-most": "^0.5.2"
  }
}
