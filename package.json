{
  "name": "@team-griffin/redux-clipboard",
  "version": "0.0.2",
  "main": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "repository": "git@github.com:team-griffin/redux-clipboard.git",
  "author": "Joshua Thornton <joshua.thornton@heg.com>",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "transpile": "babel src -d lib --copy-files",
    "lint": "npm run lint:js && npm run flow",
    "lint:js": "eslint -c ./config/.eslintrc.js 'src/**/*.js'",
    "lint:js:fix": "eslint --fix -c ./config/.eslintrc.js 'src/**/*.js'",
    "flow": "flow check; test $? -eq 0 -o $? -eq 2",
    "build": "npm run transpile",
    "test": "mocha --require babel-polyfill --compilers js:babel-register 'src/**/__tests__/*' -R spec",
    "ci": "npm run lint && npm run test && npm run build",
    "postmerge": "yarn",
    "dockerfile:ci": "dockerscript ./docker/ci/dockerfile.js ./docker/ci/Dockerfile"
  },
  "dependencies": {
    "inversify-binding-decorators": "^3.0.0",
    "inversify-vanillajs-helpers": "^2.0.0",
    "ramda": "^0.23.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.3.0"
  },
  "devDependencies": {
    "@team-griffin/eslint-config": "^1.0.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "dockerscript": "^0.0.6",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "redux-mock-store": "^1.2.2",
    "sinon": "^2.1.0"
  }
}
